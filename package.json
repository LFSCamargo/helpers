{
  "name": "builders-helpers",
  "version": "1.0.0",
  "description": "Platfom builders helpers library",
  "main": "index.js",
  "repository": "git@github.com:Platform-Builders/builders-helpers.git",
  "author": "platformbuilders",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint",
      "yarn test",
      "yarn jest:coverage",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "test": "jest --verbose",
    "jest:coverage": "jest --coverage",
    "lint": "tsc && eslint '*/**/*.{js,ts,tsx}' --fix",
    "pretty": "prettier --config ./.prettierrc.js --write \"src/**/*.js\"",
    "clean": "rm -rf ./node_modules && yarn install",
    "precommit": "yarn lint",
    "preversion": "yarn lint",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react-native": "^0.61.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/runtime": "^7.8.4",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/numeral": "^0.0.26",
    "@types/react-native": "^0.61.16",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^25.1.0",
    "jest-environment-node": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  }
}